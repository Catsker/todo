{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport AppHeader from './components/AppHeader.vue';\nimport TaskItem from './components/TaskItem.vue';\nimport AddTaskForm from './components/AddTaskForm.vue';\nimport TaskFilters from './components/TaskFilters.vue';\nimport ProgressCounter from './components/ProgressCounter.vue';\nexport default {\n  components: {\n    AppHeader,\n    TaskItem,\n    AddTaskForm,\n    TaskFilters,\n    ProgressCounter\n  },\n  data() {\n    return {\n      filter: 'all',\n      tasks: JSON.parse(localStorage.getItem('tasks')) || [{\n        id: 1,\n        text: 'Создайте первую задачу',\n        completed: false,\n        subtasks: []\n      }]\n    };\n  },\n  watch: {\n    tasks: {\n      handler() {\n        localStorage.setItem('tasks', JSON.stringify(this.tasks));\n      },\n      deep: true\n    }\n  },\n  computed: {\n    totalProgress() {\n      const completed = this.tasks.filter(task => task.completed).length;\n      return `${completed}/${this.tasks.length}`;\n    },\n    filteredTasks() {\n      switch (this.filter) {\n        case 'active':\n          return this.tasks.filter(task => !task.completed);\n        case 'completed':\n          return this.tasks.filter(task => task.completed);\n        default:\n          return this.tasks;\n      }\n    }\n  },\n  methods: {\n    createTask(text) {\n      const newId = this.tasks.length ? Math.max(...this.tasks.map(t => t.id)) + 1 : 1;\n      this.tasks.push({\n        id: newId,\n        text,\n        completed: false,\n        subtasks: []\n      });\n    },\n    createSubtask({\n      taskId,\n      text\n    }) {\n      const task = this.tasks.find(t => t.id === taskId);\n      if (!task.subtasks) task.subtasks = [];\n      const newId = task.subtasks.length ? Math.max(...task.subtasks.map(s => s.id)) + 1 : 1;\n      task.subtasks.push({\n        id: newId,\n        text,\n        completed: false\n      });\n    },\n    deleteTask(id) {\n      this.tasks = this.tasks.filter(task => task.id !== id);\n    },\n    deleteSubtask({\n      taskId,\n      subtaskId\n    }) {\n      const task = this.tasks.find(t => t.id === taskId);\n      task.subtasks = task.subtasks.filter(subtask => subtask.id !== subtaskId);\n    },\n    checkTask(task) {\n      if (task.subtasks) {\n        task.subtasks.forEach(subtask => subtask.completed = task.completed);\n      }\n    }\n  }\n};","map":{"version":3,"names":["AppHeader","TaskItem","AddTaskForm","TaskFilters","ProgressCounter","components","data","filter","tasks","JSON","parse","localStorage","getItem","id","text","completed","subtasks","watch","handler","setItem","stringify","deep","computed","totalProgress","task","length","filteredTasks","methods","createTask","newId","Math","max","map","t","push","createSubtask","taskId","find","s","deleteTask","deleteSubtask","subtaskId","subtask","checkTask","forEach"],"sources":["C:\\Users\\Pavel\\Documents\\webstorm\\todo\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"vue-cover\">\r\n    <AppHeader />\r\n    <div class=\"body\">\r\n      <TaskItem\r\n          v-for=\"task in filteredTasks\"\r\n          :key=\"task.id\"\r\n          :task=\"task\"\r\n          @delete-task=\"deleteTask\"\r\n          @check-task=\"checkTask\"\r\n          @add-subtask=\"createSubtask\"\r\n          @delete-subtask=\"deleteSubtask\"\r\n      />\r\n      <AddTaskForm @add-task=\"createTask\" />\r\n    </div>\r\n    <div class=\"footer\">\r\n      <ProgressCounter :progress=\"totalProgress\" />\r\n      <TaskFilters v-model=\"filter\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AppHeader from './components/AppHeader.vue'\r\nimport TaskItem from './components/TaskItem.vue'\r\nimport AddTaskForm from './components/AddTaskForm.vue'\r\nimport TaskFilters from './components/TaskFilters.vue'\r\nimport ProgressCounter from './components/ProgressCounter.vue'\r\n\r\nexport default {\r\n  components: { AppHeader, TaskItem, AddTaskForm, TaskFilters, ProgressCounter },\r\n  data() {\r\n    return {\r\n      filter: 'all',\r\n      tasks: JSON.parse(localStorage.getItem('tasks')) || [\r\n        { id: 1, text: 'Создайте первую задачу', completed: false, subtasks: [] }\r\n      ],\r\n    }\r\n  },\r\n  watch: {\r\n    tasks: {\r\n      handler() { localStorage.setItem('tasks', JSON.stringify(this.tasks)) },\r\n      deep: true\r\n    }\r\n  },\r\n  computed: {\r\n    totalProgress() {\r\n      const completed = this.tasks.filter(task => task.completed).length;\r\n      return `${completed}/${this.tasks.length}`;\r\n    },\r\n    filteredTasks() {\r\n      switch (this.filter) {\r\n        case 'active': return this.tasks.filter(task => !task.completed);\r\n        case 'completed': return this.tasks.filter(task => task.completed);\r\n        default: return this.tasks;\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    createTask(text) {\r\n      const newId = this.tasks.length ? Math.max(...this.tasks.map(t => t.id)) + 1 : 1;\r\n      this.tasks.push({ id: newId, text, completed: false, subtasks: [] });\r\n    },\r\n    createSubtask({ taskId, text }) {\r\n      const task = this.tasks.find(t => t.id === taskId);\r\n      if (!task.subtasks) task.subtasks = [];\r\n      const newId = task.subtasks.length ? Math.max(...task.subtasks.map(s => s.id)) + 1 : 1;\r\n      task.subtasks.push({ id: newId, text, completed: false });\r\n    },\r\n    deleteTask(id) {\r\n      this.tasks = this.tasks.filter(task => task.id !== id);\r\n    },\r\n    deleteSubtask({ taskId, subtaskId }) {\r\n      const task = this.tasks.find(t => t.id === taskId);\r\n      task.subtasks = task.subtasks.filter(subtask => subtask.id !== subtaskId);\r\n    },\r\n    checkTask(task) {\r\n      if (task.subtasks) {\r\n        task.subtasks.forEach(subtask => subtask.completed = task.completed);\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* Стили для App.vue (обёртки) */\r\n.vue-cover {\r\n  max-width: 500px;\r\n  margin: 0 auto;\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n}\r\n.body {\r\n  background-color: #FFF4E9;\r\n  padding: 30px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 25px;\r\n}\r\n.footer {\r\n  background-color: #FFCA93;\r\n  padding: 8px 30px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n</style>\r\n\r\n<!--<template class=\"vue-cover\">-->\r\n<!--  <div class=\"header\">to do list</div>-->\r\n<!--  <div class=\"body\">-->\r\n<!--    &lt;!&ndash;  TaskList  &ndash;&gt;-->\r\n<!--    <TaskList v-bind:tasks=\"tasks\" />-->\r\n<!--    &lt;!&ndash;  AddTask  &ndash;&gt;-->\r\n<!--    <AddTask/>-->\r\n<!--  </div>-->\r\n\r\n<!--  <div class=\"footer\">-->\r\n<!--    <span class=\"footer__left\">{{ totalProgress }}</span>-->\r\n<!--    <nav class=\"footer__display-tasks\">-->\r\n<!--      <input class=\"footer__display-tasks&#45;&#45;hide\" type=\"radio\" name=\"1\" id=\"all\" v-model=\"filter\" value=\"all\" checked>-->\r\n<!--      <label class=\"footer__display-tasks&#45;&#45;visible\" for=\"all\">All</label>-->\r\n<!--      <input class=\"footer__display-tasks&#45;&#45;hide\" type=\"radio\" name=\"1\" id=\"active\" v-model=\"filter\" value=\"active\">-->\r\n<!--      <label class=\"footer__display-tasks&#45;&#45;visible\" for=\"active\">Active</label>-->\r\n<!--      <input class=\"footer__display-tasks&#45;&#45;hide\" type=\"radio\" name=\"1\" id=\"completed\" v-model=\"filter\"-->\r\n<!--             value=\"completed\">-->\r\n<!--      <label class=\"footer__display-tasks&#45;&#45;visible\" for=\"completed\">Completed</label>-->\r\n<!--    </nav>-->\r\n<!--  </div>-->\r\n<!--</template>-->\r\n\r\n<!--<script>-->\r\